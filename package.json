{
  "name": "untga",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "start:development": "next dev --turbo",
    "start:production": "test -f .next/BUILD_ID || npm run build; next start",
    "start": "test -d node_modules || npm ci; npm run start:${NODE_ENV:-development}",
    "build": "prisma generate; next build",
    "fix": "next lint --fix && tsc --noEmit",
    "lint": "next lint",
    "typecheck": "tsc --noEmit",
    "migrate": "prisma migrate deploy",
    "seed": "prisma db seed",
    "test": "vitest",
    "pglite": "scripts/pglite.sh"
  },
  "prisma": {
    "seed": "tsx prisma/seed.ts"
  },
  "dependencies": {
    "@prisma/client": "^6.8.2",
    "@tanstack/react-query": "^5.76.1",
    "@tanstack/react-query-devtools": "^5.76.1",
    "bcrypt": "^6.0.0",
    "lucide-react": "^0.511.0",
    "luxon": "^3.6.1",
    "next": "15.3.2",
    "proxy-addr": "^2.0.7",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "server-only": "^0.0.1",
    "tailwind-merge": "^3.3.0",
    "tailwind-variants": "^1.0.0",
    "ua-parser-js": "^2.0.3",
    "zod": "^4.0.0-beta.20250505T195954"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3.3.1",
    "@playwright/test": "^1.52.0",
    "@tailwindcss/postcss": "^4.1.7",
    "@types/bcrypt": "^5.0.2",
    "@types/luxon": "^3.6.2",
    "@types/node": "^22.15.18",
    "@types/proxy-addr": "^2.0.3",
    "@types/react": "^19.1.4",
    "@types/react-dom": "^19.1.5",
    "eslint": "^9.27.0",
    "eslint-config-next": "^15.3.2",
    "postcss": "^8.5.3",
    "prisma": "^6.8.2",
    "prismock": "^1.35.3",
    "tailwindcss": "^4.1.7",
    "tsx": "^4.19.4",
    "typescript": "^5.8.3",
    "vite-tsconfig-paths": "^5.1.4",
    "vitest": "^3.1.3"
  }
}
