{
  "name": "untga",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "prestart:development": "test -d node_modules || npm ci; test -d prisma/client || prisma generate",
    "start": "npm run start:${NODE_ENV:-development}",
    "start:development": "next dev --turbo",
    "prestart:production": "prisma migrate deploy",
    "start:production": "next start",
    "prebuild": "prisma generate",
    "build": "next build",
    "fix": "next lint --fix && tsc --noEmit",
    "lint": "next lint",
    "typecheck": "tsc --noEmit",
    "test": "vitest"
  },
  "prisma": {
    "seed": "tsx prisma/seed.ts"
  },
  "dependencies": {
    "@prisma/client": "^6.7.0",
    "@tanstack/react-query": "^5.75.5",
    "@tanstack/react-query-devtools": "^5.75.5",
    "bcrypt": "^5.1.1",
    "lucide-react": "^0.508.0",
    "luxon": "^3.6.1",
    "next": "15.3.2",
    "proxy-addr": "^2.0.7",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "server-only": "^0.0.1",
    "tailwind-merge": "^3.2.0",
    "tailwind-variants": "^1.0.0",
    "ua-parser-js": "^2.0.3",
    "zod": "^4.0.0-beta.20250505T195954"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3.3.1",
    "@tailwindcss/postcss": "^4.1.5",
    "@types/bcrypt": "^5.0.2",
    "@types/luxon": "^3.6.2",
    "@types/node": "^22.15.15",
    "@types/proxy-addr": "^2.0.3",
    "@types/react": "^19.1.3",
    "@types/react-dom": "^19.1.3",
    "eslint": "^9.26.0",
    "eslint-config-next": "^15.3.2",
    "postcss": "^8.5.3",
    "prisma": "^6.7.0",
    "prismock": "^1.35.3",
    "tailwindcss": "^4.1.5",
    "tsx": "^4.19.4",
    "typescript": "^5.8.3",
    "vite-tsconfig-paths": "^5.1.4",
    "vitest": "^3.1.3"
  }
}
