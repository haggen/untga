{
  "name": "untga",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "start": "npm run-script start:${NODE_ENV:-development}",
    "start:development": "next dev",
    "prestart:development": "test -d node_modules || npm ci",
    "prebuild": "prisma generate",
    "build": "next build",
    "start:production": "next start",
    "fix": "next lint --fix",
    "lint": "next lint",
    "typecheck": "tsc --noEmit",
    "test": "playwright test"
  },
  "prisma": {
    "seed": "node --experimental-strip-types prisma/seed.ts"
  },
  "dependencies": {
    "@prisma/client": "^6.3.1",
    "bcrypt": "^5.1.1",
    "luxon": "^3.5.0",
    "next": "15.1.7",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "tailwind-merge": "^3.0.1",
    "tailwind-variants": "^0.3.1",
    "zod": "^3.24.2"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@tailwindcss/postcss": "^4.0.6",
    "@types/bcrypt": "^5.0.2",
    "@types/luxon": "^3.4.2",
    "@types/node": "^22",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "eslint": "^9",
    "eslint-config-next": "15.1.7",
    "postcss": "^8",
    "prisma": "^6.3.1",
    "tailwindcss": "^4.0.6",
    "typescript": "^5"
  }
}
